
case $config_platform_branch in
  *-cec3)
    ;;
  *)
    return
    ;;
esac

cd $config_platform_install/build
echo "Installing platform package"
$config_build_env make install

rm -fr $(readlink -f $XBIANARCHDIR/working/debian/tmp)
mkdir -p $(readlink -f $XBIANARCHDIR/working/debian/tmp)
mv $DESTDIR/usr $(readlink -f $XBIANARCHDIR/working/debian/tmp)
